{
	"name": "nimius/workshops",
	"type": "typo3-cms-extension",
	"description": "Flexible workshop management",
	"keywords": [
		"TYPO3", "extension", "workshops", "courses", "seminars"
	],
	"homepage": "https://github.com/nimius/workshops",
	"authors": [
		{
			"name": "NIMIUS",
			"role": "Developer",
			"homepage": "http://nimius.net"
		}
	],
	"repositories": [
		{
			"type": "composer",
			"url": "https://composer.typo3.org"
		}
	],
	"license": "GPL-2.0+",
	"require": {
		"typo3/cms-core": ">=8.7.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.8.0",
        "nimut/testing-framework": "^1.1"
	},
	"replace": {
		"workshops": "self.version",
		"typo3-ter/workshops": "self.version"
	},
	"config": {
		"vendor-dir": ".Build/vendor",
		"bin-dir": ".Build/bin"
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": ".Build/Web"
		}
	},
	"autoload": {
		"psr-4": {
			"NIMIUS\\Workshops\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"NIMIUS\\Workshops\\Tests\\": "Tests"
		}
	}
}
