<?xml version="1.0" encoding="UTF-8"?>
<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<workshops>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:workshops/Resources/Private/Language/locallang.xlf:flexForm.sheets.workshops</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.categoryOperator>
						<TCEforms>
							<label>LLL:EXT:workshops/Resources/Private/Language/locallang.xlf:flexForm.settings.categoryOperator</label>
							<onChange>reload</onChange>
							<config>
								<type>select</type>
								<items>
									<numIndex index="0" type="array">
										<numIndex index="0">LLL:EXT:workshops/Resources/Private/Language/locallang.xlf:flexForm.settings.categoryOperator.none</numIndex>
										<numIndex index="1"></numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:workshops/Resources/Private/Language/locallang.xlf:flexForm.settings.categoryOperator.and</numIndex>
										<numIndex index="1">AND</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:workshops/Resources/Private/Language/locallang.xlf:flexForm.settings.categoryOperator.or</numIndex>
										<numIndex index="1">OR</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</settings.categoryOperator>
					
					<settings.categories>
						<TCEforms>
							<label>LLL:EXT:workshops/Resources/Private/Language/locallang.xlf:flexForm.settings.categories</label>
							<displayCond>FIELD:settings.categoryOperator:REQ:true</displayCond>
							<config>
								<type>select</type>
								<renderMode>tree</renderMode>
								<treeConfig>
									<parentField>parent</parentField>
									<appearance>
										<maxLevels>99</maxLevels>
										<expandAll>TRUE</expandAll>
										<showHeader>TRUE</showHeader>
									</appearance>
								</treeConfig>
								<foreign_table>sys_category</foreign_table>
								<foreign_table_where> AND (sys_category.sys_language_uid = 0 OR sys_category.l10n_parent = 0) ORDER BY sys_category.sorting</foreign_table_where>
								<size>10</size>
								<autoSizeMax>20</autoSizeMax>
								<minitems>0</minitems>
								<maxitems>99</maxitems>
							</config>
						</TCEforms>
					</settings.categories>

					<settings.detailPage>
						<TCEforms>
							<label>LLL:EXT:workshops/Resources/Private/Language/locallang.xlf:flexForm.settings.detailPage</label>
							<config>
								<type>input</type>
								<eval>trim</eval>
								<size>10</size>
								<wizards type="array">
								    <_PADDING type="integer">2</_PADDING>
								    <link type="array">
								      <type>popup</type>
								      <title>Link</title>
								      <icon>link_popup.gif</icon>
									  <module type="array">
									  	<name>wizard_element_browser</name>
									  	<urlParameters type="array">
									  		<mode>wizard</mode>
									  	</urlParameters>
									  </module>
								      <JSopenParams>height=500,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
								    </link>
								  </wizards>     
							</config>
						</TCEforms>
					</settings.detailPage>
					
					<settings.sortingField>
						<TCEforms>
							<label>LLL:EXT:workshops/Resources/Private/Language/locallang.xlf:flexForm.settings.sortingField</label>
							<config>
								<type>select</type>
								<items>
									<numIndex index="0" type="array">
										<numIndex index="0">LLL:EXT:workshops/Resources/Private/Language/locallang.xlf:flexForm.settings.sortingField.sorting</numIndex>
										<numIndex index="1">sorting</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:workshops/Resources/Private/Language/locallang.xlf:flexForm.settings.sortingField.identifier</numIndex>
										<numIndex index="1">identifier</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:workshops/Resources/Private/Language/locallang.xlf:flexForm.settings.sortingField.name</numIndex>
										<numIndex index="1">name</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
										<numIndex index="0">LLL:EXT:workshops/Resources/Private/Language/locallang.xlf:flexForm.settings.sortingField.dates_beginAt</numIndex>
										<numIndex index="1">dates.beginAt</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</settings.sortingField>

					<settings.sortingType>
						<TCEforms>
							<label>LLL:EXT:workshops/Resources/Private/Language/locallang.xlf:flexForm.settings.sortingType</label>
							<config>
								<type>select</type>
								<items>
									<numIndex index="0" type="array">
										<numIndex index="0">LLL:EXT:workshops/Resources/Private/Language/locallang.xlf:flexForm.settings.sortingType.ascending</numIndex>
										<numIndex index="1">ASC</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:workshops/Resources/Private/Language/locallang.xlf:flexForm.settings.sortingType.descending</numIndex>
										<numIndex index="1">DESC</numIndex>
									</numIndex>
								</items>
							</config>
						</TCEforms>
					</settings.sortingType>
					
					<settings.hideWorkshopsWithoutUpcomingDates>
						<TCEforms>
							<label>LLL:EXT:workshops/Resources/Private/Language/locallang.xlf:flexForm.settings.hideWorkshopsWithoutUpcomingDates</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</settings.hideWorkshopsWithoutUpcomingDates>
				</el>
			</ROOT>
		</workshops>
		
		<dates>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:workshops/Resources/Private/Language/locallang.xlf:flexForm.sheets.dates</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.hideAlreadyStartedDates>
						<TCEforms>
							<label>LLL:EXT:workshops/Resources/Private/Language/locallang.xlf:flexForm.settings.hideAlreadyStartedDates</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</settings.hideAlreadyStartedDates>

					<settings.hidePastDates>
						<TCEforms>
							<label>LLL:EXT:workshops/Resources/Private/Language/locallang.xlf:flexForm.settings.hidePastDates</label>
							<config>
								<type>check</type>
							</config>
						</TCEforms>
					</settings.hidePastDates>
                    
					<settings.upcomingDays>
						<TCEforms>
							<label>LLL:EXT:workshops/Resources/Private/Language/locallang.xlf:flexForm.settings.upcomingDays</label>
							<config>
								<type>input</type>
								<eval>trim,num</eval>
								<default></default>
								<size>4</size>
							</config>
						</TCEforms>
					</settings.upcomingDays>
				</el>
			</ROOT>
		</dates>
	</sheets>
</T3DataStructure>